#! groovy
pipeline { 
agent any
stages {
    stage ( "QA" ) 
    echo 'hello world'
    sh '''#!/bin/bash
# declare STRING variable
STRING="Hello World"
STRING="THERE IS A NEW AUTOMATION TOOL"
#print variable on a screen
echo $STRING'''
stage ('DEV') 
    echo 'who am i'
    sh '''#!/bin/bash
STRING="CLEMENT SHAKIRAT"
#print variable on a screen
echo $STRING'''

}

}
